mydb <- dbConnect(MySQL(), user='root', dbname='ventas', host='192.168.1.4')
query <- paste("SELECT idOrden cantidad, datediff(now(), vo.fechaHora) diasDemora, fx_etapaOrdenWeb(idOrden) etapaOrden, idMarketplace FROM ventas.ventaweb_orden vo
WHERE fx_etapaOrdenWeb(idOrden) IN ('Articulos pedidos', 'Articulos sin pedir', 'Enviada', 'Facturada')
AND idOrden NOT IN (SELECT idOrden FROM ventaweb_orden_enviada)
AND fechaHora > '2019-05-01'
AND vo.estadoOrden IN ('Facturada', 'Abonado', 'Aprobado')")
interdepositos <-dbGetQuery(mydb, query)
dbDisconnect(mydb)
